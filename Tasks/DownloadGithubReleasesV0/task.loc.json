{
  "id": "263abc27-4582-4174-8789-af599697778e",
  "name": "DownloadGithubReleases",
  "friendlyName": "ms-resource:loc.friendlyName",
  "description": "ms-resource:loc.description",
  "author": "Microsoft Corporation",
  "helpMarkDown": "ms-resource:loc.helpMarkDown",
  "category": "Utility",
  "visibility": [
    "Build",
    "Release"
  ],
  "demands": [],
  "version": {
    "Major": 0,
    "Minor": 145,
    "Patch": 0
  },
  "minimumAgentVersion": "1.99.0",
  "instanceNameFormat": "ms-resource:loc.instanceNameFormat",
  "inputs": [
    {
      "name": "connection",
      "type": "connectedService:github",
      "label": "ms-resource:loc.input.label.connection",
      "defaultValue": "",
      "required": true,
      "helpMarkDown": "ms-resource:loc.input.help.connection"
    },
    {
      "name": "definition",
      "type": "pickList",
      "label": "ms-resource:loc.input.label.definition",
      "defaultValue": "",
      "required": true,
      "properties": {
        "EditableOptions": "True"
      },
      "helpMarkDown": "ms-resource:loc.input.help.definition"
    },
    {
      "name": "releaseByTagName",
      "type": "boolean",
      "label": "ms-resource:loc.input.label.releaseByTagName",
      "defaultValue": "false",
      "required": false,
      "properties": {
        "EditableOptions": "True"
      },
      "helpMarkDown": "ms-resource:loc.input.help.releaseByTagName"
    },
    {
      "name": "versions",
      "type": "pickList",
      "label": "ms-resource:loc.input.label.versions",
      "defaultValue": "",
      "required": true,
      "properties": {
        "EditableOptions": "True"
      },
      "helpMarkDown": "ms-resource:loc.input.help.versions"
    },
    {
      "name": "itemPattern",
      "type": "string",
      "label": "ms-resource:loc.input.label.itemPattern",
      "defaultValue": "**",
      "required": false,
      "helpMarkDown": "ms-resource:loc.input.help.itemPattern"
    },
    {
      "name": "downloadPath",
      "type": "string",
      "label": "ms-resource:loc.input.label.downloadPath",
      "defaultValue": "$(System.ArtifactsDirectory)",
      "required": true,
      "helpMarkDown": "ms-resource:loc.input.help.downloadPath"
    }
  ],
  "dataSourceBindings": [
    {
      "target": "definition",
      "endpointId": "$(connection)",
      "dataSourceName": "UserRepositories",
      "resultTemplate": "{ \"Value\" : \"{{full_name}}\", \"DisplayValue\" : \"{{full_name}}\" }"
    },
    {
      "target": "versions",
      "endpointId": "$(connection)",
      "dataSourceName": "Releases",
      "resultTemplate": "{ \"Value\" : \"{{id}}\", \"DisplayValue\" : \" {{#equals releaseByTagName 'true'}}{{#if tag_name}}{{tag_name}}{{else}}{{id}}{{/if}}{{else}}{{#if name}}{{name}}{{else}}{{id}}{{/if}}{{/equals}}\" }",
      "parameters": {
        "repositoryName": "$(definition)",
        "releaseByTagName": "$(releaseByTagName)"
      }
    }
  ],
  "execution": {
    "Node": {
      "target": "main.js",
      "argumentFormat": ""
    }
  },
  "messages": {
    "DownloadArtifacts": "ms-resource:loc.messages.DownloadArtifacts",
    "ArtifactsSuccessfullyDownloaded": "ms-resource:loc.messages.ArtifactsSuccessfullyDownloaded"
  }
}