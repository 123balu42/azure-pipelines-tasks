      "properties": {
        "EditableOptions": "True"
      }
    },
    {
      "name": "architecture",
      "type": "pickList",
      "label": "ms-resource:loc.input.label.architecture",
      "defaultValue": "amd64",
      "required": false,
      "options": {
        "amd64": "amd64",
        "arm": "arm",
        "x86": "x86"
      },
      "helpMarkDown": "ms-resource:loc.input.help.architecture",
      "groupName": "platform",
      "properties": {
        "EditableOptions": "True"
      }
    },
    {
